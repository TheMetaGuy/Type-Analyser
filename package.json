{
  "name": "type-master",
  "version": "1.2.1",
  "description": "utility package designed to provide in-depth and precise type information for any JavaScript object, be it a primitive, built-in types, ES6/ES2020 types or custom classes",
  "main": "dist/type-master.cjs.js",
  "module": "dist/type-master.esm.js",
  "browser": "dist/type-master.iife.min.js",
  "files"  : [
    "dist/"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c"
  },
  "keywords": [
    "types",
    "typeof",
    "type-introspection",
    "type-check",
    "type-checker",
    "type-details",
    "JSON-serialize-check",
    "isNaN",
    "is-safe-number",
    "number-type"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theMetaGuy/type-master.git"
  },
  "author": { 
    "name" : "Owen Cullum",
    "email" : "dev@metagu.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "rollup": "^3.25.1"
  }
}
